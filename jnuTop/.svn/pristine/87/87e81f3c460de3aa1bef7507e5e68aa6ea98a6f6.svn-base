<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.stylefeng.guns.modular.business.dao.ExcelModelDao">
	
	<select id="countModel" parameterType="String" resultType="Integer" >
		select COUNT(model_name) as result from t_excelmodel where model_name = #{modelName}
	</select>
	
	<select id="getDbModel"  parameterType="String"  resultType="java.util.Map"  statementType="STATEMENT">  
     	select * from  ${tableName} limit 0,1
	</select>  
</mapper>