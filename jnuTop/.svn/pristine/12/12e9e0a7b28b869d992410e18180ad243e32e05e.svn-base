@layout("/common/_container.html"){
<div class="row">
    <div class="col-sm-12">
        <div class="ibox float-e-margins">
            <div class="ibox-title">
                <h5>样本领用管理</h5>
            </div>
            <div class="ibox-content">
                <div class="row row-lg">
                    <div class="col-sm-12">
                        <div class="row">
                            <div class="col-sm-3">
                                <#NameCon id="condition" name="名称" />
                            </div>
                            <div class="col-sm-3">
                                <#button name="搜索" icon="fa-search" clickFun="SampleUsed.search()"/>
                            </div>
                        </div>
                        <div class="hidden-xs" id="SampleUsedTableToolbar" role="group">
                            @if(shiro.hasPermission("/sampleUsed/add")){
                                <#button name="领用" icon="fa-plus" clickFun="SampleUsed.openAddSampleUsed()"/>
                            @}
                            @if(shiro.hasPermission("/sampleUsed/check")){
                                <#button name="查看领用记录" icon="fa-plus" clickFun="SampleUsed.openSampleUsedDetail(-1)" space="true"/>
                            @}
                             @if(shiro.hasPermission("/sampleUsed/importExcel")){
                                <#button name="导入excel" icon="fa-plus" clickFun="SampleUsed.uploadFile()" space="true"/>
                            @}
                          
                        </div>
                        <#table id="SampleUsedTable"/>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<script src="${ctxPath}/static/modular/business/sampleUsed/sampleUsed.js"></script>
@}
